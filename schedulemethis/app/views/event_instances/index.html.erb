<p id="notice"><%= notice %></p>

<h1>Listing Event Instances</h1>

<table>
  <thead>
    <tr>
      <th>Event</th>
      <th>Completed</th>
      <th>Dismissed</th>
      <th>Scheduled start</th>
      <th>Scheduled end</th>
      <th>Location</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @event_instances.each do |event_instance| %>
      <tr>
        <td><%= event_instance.event %></td>
        <td><%= event_instance.completed %></td>
        <td><%= event_instance.dismissed %></td>
        <td><%= event_instance.scheduled_start %></td>
        <td><%= event_instance.scheduled_end %></td>
        <td><%= event_instance.location %></td>
        <td><%= link_to 'Show', event_instance %></td>
        <td><%= link_to 'Edit', edit_event_instance_path(event_instance) %></td>
        <td><%= link_to 'Destroy', event_instance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event instance', new_event_instance_path %>
